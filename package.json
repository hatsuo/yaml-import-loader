{
  "name": "yaml-import-loader",
  "version": "1.1.1",
  "description": "YAML loader for Webpack supporting the '!import <file>' type to include different YAML files, and '!import-raw <file>' to include the raw contents of a file.",
  "main": "index.js",
  "scripts": {
    "start": "npm run test:watch",
    "test": "mocha test/**/*.spec.js",
    "test:watch": "mocha --watch --watch-extensions js,yml,yaml,json test/**/*.spec.js",
    "preversion": "npm run test",
    "prepublishOnly": "npm run preversion"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngfk/yaml-import-loader.git"
  },
  "keywords": [
    "yml",
    "yaml",
    "import",
    "loader",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngfk/yaml-import-loader/issues"
  },
  "homepage": "https://github.com/ngfk/yaml-import-loader#readme",
  "dependencies": {
    "js-yaml": "^3.8.4",
    "loader-utils": "^1.1.0"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "chai": "^4.0.2",
    "mocha": "^3.4.2"
  }
}
